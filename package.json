    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.27",
    "@types/uuid": "^9.0.8",
    "esbuild": "^0.20.2",
    "esbuild-node-externals": "^1.13.0",
    "fs-extra": "^11.2.0",
